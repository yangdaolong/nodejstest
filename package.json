{
  "name": "nodejstest",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "cross-env NODE_ENV=dev node bin/www",
    "dev": "cross-env NODE_ENV=dev PORT=3001 ./node_modules/.bin/nodemon bin/www",
    "prd": "cross-env NODE_ENV=prd pm2 start bin/www --name nodejstest",
    "test": "echo \"Error: no test specified\" && exit 1",
    "prisma db pull": "npx prisma db pull",
    "npx prisma db push": "npx prisma db push",
    "prisma generate": "npx prisma generate",
    "Prisma Migrate step 1": "npx prisma migrate dev --create-only",
    "Prisma Migrate step 2": "prisma migrate dev"
  },
  "dependencies": {
    "@prisma/client": "^5.8.1",
    "dayjs": "^1.11.10",
    "debug": "^4.1.1",
    "dotenv": "^16.4.1",
    "ejs": "~2.3.3",
    "koa": "^2.7.0",
    "koa-bodyparser": "^4.2.1",
    "koa-convert": "^1.2.0",
    "koa-json": "^2.0.2",
    "koa-logger": "^3.2.0",
    "koa-onerror": "^4.1.0",
    "koa-router": "^7.4.0",
    "koa-static": "^5.0.0",
    "koa-views": "^6.2.0",
    "moment": "^2.30.1",
    "pg": "^8.11.3",
    "pg-hstore": "^2.3.4",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "cross-env": "^7.0.3",
    "nodemon": "^1.19.1",
    "prisma": "^5.8.1"
  }
}